
package sample.epackage

  enumeration type Other
    @|epackage/Other.barlom|

  enumeration type Color

    symbol BLUE : "blue",
    symbol GREEN,
    symbol RED,
    symbol YELLOW,

    function isCold() : Boolean :
      match self
        BLUE => return true
        GREEN => return true
      else
        return false
      end
    end

  end

end