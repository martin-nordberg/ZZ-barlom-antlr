

package sample.integer_literals;


// Integer literals (zero)

constant zint0 = 0;

constant zint2 = 0x0;

constant zint3 = 0b0;

constant zint4 = 00;

constant zint5 = 0x0000;

constant zint6 = 0b0000_0000;


// Integer literals (non-zero)

constant int1 = 123_456;

constant int2 = 0b1010101000011;

constant int3 = 0xABCD_ef01;


// Integer literals (long)

constant lint1 = 123456789L;

constant lint2 = 0xCAFE_BABE_1234_0000l;

constant lint3 = 2L;



