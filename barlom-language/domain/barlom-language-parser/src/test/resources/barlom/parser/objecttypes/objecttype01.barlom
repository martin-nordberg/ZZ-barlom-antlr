
use lib.something.BaseObject

object type stuff.specialstuff.MyObject(
    a : transient : Integer32,
    b : public : Text,
    c : public : Integer32 )
  : "an example of a simple object type"
  : extends( BaseObject )

  constant d = a^2

  variable e = a * c

  function f( x : Integer32 ) : Integer32
    return e + c*x
  end

  function name() : Text
    return self.b
  end

end