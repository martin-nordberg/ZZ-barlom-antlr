

module stuff.specialstuff@1.1.0
  : contracted
    dependencies( [
      lib.gunk@2.1.1
    ] )
    documented
    paradigms( [
      algebraic,
      annotational,
      functional,
      graph_oriented,
      logic,
      object_oriented,
      structural,
      tree_oriented
    ] )
    tested
    type_checked
    versioned
begin

  synonym extra = lib.gunk.extra@2.1.1;

  function doStuff@1.0.1( x , y ) begin
    return x + y;
  end

  function doStuff@1.1.0( x , y ) begin
    return x + y + extra( x, y );
  end

  function factorial( n : Integer32 )
    : "The factorial function (@1.0.0 implicitly) is defined in recursive fragments.";
  function factorial( 0 )
    : "The base case"
    return 1;
  function factorial( n )
    : "Tail recursion"
    return n * factorial( n - 1 );

end


module stuff.specialstuff@2.0.0 begin

  function doMore@2.0.0( a ) begin
    return a^2;
  end

end