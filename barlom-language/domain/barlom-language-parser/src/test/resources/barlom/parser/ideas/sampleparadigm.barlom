
namespace stuff;

module specialstuff
  : contracted
    dependencies( [
      lib.gunk version( 2.1.1 )
    ] )
    documented
    paradigms( [
      algebraic,
      annotational,
      functional,
      graph_oriented,
      logic,
      object_oriented,
      structural,
      tree_oriented
    ] )
    tested
    type_checked
    version( 1.1.0 )
begin

  function extra synonym_for lib.gunk.extra : version( 2.1.1 );

  function doStuff@1.0.1( x , y ) begin
    return x + y;
  end

  function doStuff@1.1.0( x , y ) begin
    return x + y + extra( x, y );
  end

  function factorial( n : Integer32 )
    : "The factorial function (1.0.0 implicitly) is defined in recursive fragments.";
      recursive
    return n * factorial( n - 1 );

  function factorial( 0 )
    : "The base case"
    return 1;

end


module specialstuff@2.0.0 begin

  function doMore@2.0.0( a ) begin
    return a^2;
  end

end