
use lib.something.BaseStructure

structure type stuff.specialstuff.MyStructure(
    a : transient Integer32,
    b : public Text,
    c : public Integer32 )
  : "an example of a simple structure type"
    extends( BaseStructure ) :

  constant d = a^2

  variable e = a * c

  function f( x : Integer32 ) : Integer32 :
    return e + c*x
  end

  function name() : Text :
    return self.b
  end

end