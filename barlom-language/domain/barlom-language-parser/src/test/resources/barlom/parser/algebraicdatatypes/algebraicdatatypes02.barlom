
algebraic data type sample.mypackage.List( T )

  variant Nil,

  variant Cons( head : T, tail : List(T) )

  function head() : T :
    match self
      Nil => return Nil
      Cons( head, tail ) => return head
    end
  end

  function tail() : List(T) :
    match self
      Nil => return Nil
      Cons( head, tail ) => return tail
    end
  end

end